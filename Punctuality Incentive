import java.util.*;
public class Hello{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=0;
        int count=0;
        while(count<b){
            c+=(a+count*200);
            count++;
        }
        System.out.println(c);
    }
}
