import java.util.*;
public class Hello {
    public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int a=sc.nextInt();
	int x=a;
	sc.nextLine();
	int first=0;
	int second=0;
	while(x-->0){
	    String[] str=sc.nextLine().split(" ");
	    first+=Integer.parseInt(str[0]);
	    second+=Integer.parseInt(str[1]);
	}
	System.out.printf("%.2f\n%.2f",(float)first/a,(float)second/a);
	}
}
