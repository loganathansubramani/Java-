import java.util.*;
public class Hello {
    public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	int a=sc.nextInt();
	int b=sc.nextInt();
	int c=sc.nextInt();
	int small=a;
	if(b<small){
	    small=b;
	}
	if(c<small){
	    small=c;
	    }
	    System.out.print(small);
	}
}
