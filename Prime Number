import java.util.*;
public class Hello{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=a;
        int count=0;
        while(b>1){
            if(a%b==0){
                count++;
            }
            b--;
        }
        if(count<2){
            System.out.print("YES");
        }
        else{
            System.out.print("NO");
        }
    }
}
