import java.util.*;
public class Hello {
    public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int a=sc.nextInt();
	sc.nextLine();
	int val=0;
	String str="";
	String batsman="";
	while(a-->0){
	    batsman=sc.nextLine();
	    String[] arr=batsman.split(",");
	    int x=Integer.parseInt(arr[1]);
	    if(x>val){
	        val=x;
	        str=arr[0];
	    }
	}
	System.out.print(str);
	}
}
