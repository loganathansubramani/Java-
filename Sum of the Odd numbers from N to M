import java.util.*;
public class Hello {
    public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int a=sc.nextInt();
    int b=sc.nextInt();
    int count=0;
    while(a<=b){
        if(a%2!=0){
        count+=a;
        }a++;
    }
    System.out.print(count);
	}
}
