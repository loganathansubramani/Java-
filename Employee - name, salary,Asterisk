import java.util.*;
public class Hello{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        String a = sc.next();
        int b= sc.nextInt();
        double c = sc.nextDouble();
        System.out.println(a);
        System.out.println("**");
        System.out.println(b);
        System.out.println("**");
        System.out.println(c);
        }
}
