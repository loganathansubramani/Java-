import java.util.*;
public class Hello {
    public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int a=sc.nextInt();
    int sum=0;
    while(a>0){
        int c=a%10;
        if(c%2==1){
            sum+=c;
        }
            a=a/10;
    }System.out.print(sum);
	}
}
