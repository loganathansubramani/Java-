import java.util.Scanner; 
public class Hello { 
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in); 
        String str1 = sc.nextLine().toLowerCase(); 
        String str2 = sc.nextLine().toLowerCase(); 
        if (str1.equals(str2)) { 
            System.out.print("YES"); 
        } else { 
            System.out.print("NO");
        }
    } 
}
