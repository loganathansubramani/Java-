import java.util.Scanner;

public class Hello {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String str1 = sc.next();
        String str2 = sc.next();
        
        // Check if string2 is present in string1
        if (str1.contains(str2)) {
            System.out.print("Yes");
        } else {
            System.out.print("No");
        }
    }
}
