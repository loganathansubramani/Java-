import java.util.*;
public class Hello{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        String str=sc.nextLine();
        char[] ch=str.toCharArray();
        for(int i=0;i<ch.length;i++){
            if(i!=0){
                if(i!=ch.length-1){
                    System.out.print(ch[i]);
                }
            }
        }
    }
}
