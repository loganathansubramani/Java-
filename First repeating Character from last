import java.util.*;
public class Hello {
    public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	String str=sc.nextLine();
	outer:
	for(int i=str.length()-1;i>=0;i--)
	{
	    for(int j=i-1;j>=0;j--)
	    {
	        if(str.charAt(i)==str.charAt(j))
	        {
	            System.out.print(str.charAt(i));
	            break outer;
	        }
	    }
	}
	}
}
